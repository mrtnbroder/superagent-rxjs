{
  "name": "superagent-rxjs",
  "version": "2.1.0",
  "description": "Superagent results wrapped within an RxJS 5 Observable",
  "main": "lib/index",
  "module": "es6/index",
  "jsnext:main": "es6/index",
  "scripts": {
    "build": "npm run cleanup && npm run build-cjs && npm run build-es",
    "build-cjs": "BABEL_ENV=cjs babel ./src -d lib --ignore '__tests__'",
    "build-es": "BABEL_ENV=es babel ./src -d es6 --ignore '__tests__'",
    "build-min": "NODE_ENV=production webpack -p src/index.js umd/VimcarAPI.min.js",
    "build-umd": "NODE_ENV=development webpack src/index.js umd/VimcarAPI.js",
    "ci-test": "npm run typecheck",
    "cleanup": "rm -rf es6 lib umd",
    "coverage": "nyc report --cache --reporter=text-lcov | coveralls",
    "release": "npm run build && standard-version",
    "test": "ava",
    "typecheck": "flow check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrtnbroder/superagent-rxjs.git"
  },
  "keywords": [
    "async",
    "fp",
    "frp",
    "functional",
    "observable",
    "reactive",
    "request",
    "rx",
    "rxjs",
    "rxnext",
    "superagent",
    "xhr"
  ],
  "engines": {
    "node": ">=4.0"
  },
  "author": "Martin Broder <hello@martinbroder.com> (https://martinbroder.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mrtnbroder/superagent-rxjs/issues"
  },
  "homepage": "https://github.com/mrtnbroder/superagent-rxjs#readme",
  "dependencies": {
    "rxjs": "^5.0.0-beta.0"
  },
  "devDependencies": {
    "ava": "0.16.0",
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-flow-comments": "6.3.19",
    "babel-preset-es2015": "6.16.0",
    "babel-register": "6.16.3",
    "coveralls": "2.11.14",
    "flow-bin": "0.33.0",
    "nock": "8.0.0",
    "nyc": "8.3.0",
    "superagent": "2.3.0",
    "webpack": "2.1.0-beta.25"
  },
  "peerDependencies": {
    "superagent": "^2.0.0"
  },
  "files": [
    "*.md",
    "es6",
    "lib",
    "umd"
  ],
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  }
}
